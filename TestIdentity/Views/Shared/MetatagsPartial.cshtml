		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		@if(ViewBag.Title != null){
			<title>Tittle = @ViewBag.Title</title>
		}
		else{
			<title>Наш сайт</title>
		}
		@if(ViewBag.Description != null){
			<meta name="description" content="@ViewBag.Description" /> //мета для описания страницы
		}
		@if(ViewBag.Keywords != null){
			<meta name="keywords" content="@ViewBag.Keywords" />  //мета для ключевых слов
		}
